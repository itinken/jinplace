	},
	"keywords": [
		"jquery-plugin",
		"inline",
		"edit",
		"form"
	],
	"author": {
		"name": "Steve Ratcliffe"
	},
	"licenses": [
		{
			"type": "MIT",
			"url": "http://www.opensource.org/licenses/mit-license.php"
		}
	],

	"homepage": "https://bitbucket.org/itinken/jinplace",
	"bugs": "https://bitbucket.org/itinken/jinplace/issues",

	"dependencies": {
